{
    "name": "jdenoc/laravel-make-trait",
    "description": "artisan make:trait",
    "homepage": "https://github.com/jdenoc/laravel-make-trait",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "artisan",
        "trait"
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Jdenoc\\MakeTrait\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jdenoc\\MakeTrait\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
              "Jdenoc\\MakeTrait\\MakeTraitServiceProvider"
            ]
        }
    }
}
